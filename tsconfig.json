{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2020",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true,
    "declaration": false
  }
}
